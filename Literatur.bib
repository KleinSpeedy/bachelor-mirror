@book{RJEthernet,
abstract = {Bei Ethernet handelt es sich um eine Standard-Netzwerktechnologie für kabelgebundene Datennetze die dafür sorgt, dass alle Geräte in einem lokalen Datennetz miteinander kommunizieren können. Das Handbuch von Rech behandelt die Themen Standard-Ethernet, Fast Ethernet, Gigabit-Ethernet und 10Gigabit-Ethernet. Für diese neue Auflage hat er sein Vorgängerwerk (ID-G 5/08) überarbeitet und aktualisiert. Hinzugekommen ist ein neues Kapitel zu 40/100Gigabit-Ethernet. Mithilfe dieser Standarderweiterung kann die Datenrate noch einmal um das 4- oder 10-fache gesteigert werden. Weitere Ergänzungen betreffen die Leistungserweiterung bei der Stromversorgung über das Twisted-Pair-Kabel und Anpassungen bei der Energieeffizienz. Entfallen ist dagegen die Einführung in die Novell-Protokollfamilie. Dieses überzeugende Arbeitsbuch für Netzwerkprofis, Studenten und Auszubildende im Bereich Netzwerktechnik wird wiederum ergänzt durch eine Übersicht der verwendeten Abkürzungen und durch ein aktualisiertes Literaturverzeichnis. Vorgänger ersetzen. Passendes Pendant zur WLAN-Darstellung des Autors (ID-G 32/12). (3)},
author = {Rech, Jörg},
address = {Hannover},
booktitle = {Ethernet :  Technologien und Protokolle für die Computervernetzung ; [Standard-Ethernet ; Fast Ethernet ; Gigabit-Ethernet ; 10Gigabit-Ethernet ; 40/100Gigabit-Ethernet ; PoE und PoE+] /},
edition = {3., aktualisierte und erw. Aufl.},
isbn = {9783944099040},
keywords = {Ethernet},
language = {ger},
publisher = {: Heise-Zeitschriften-Verl.},
title = {Ethernet :  Technologien und Protokolle für die Computervernetzung ; [Standard-Ethernet ; Fast Ethernet ; Gigabit-Ethernet ; 10Gigabit-Ethernet ; 40/100Gigabit-Ethernet ; PoE und PoE+] / },
year = {2014},
}

@incollection{MasterkursVirtParaSys_Virt,
abstract = {Virtualisierung ist eine Herangehensweise in der Informationstechnologie, die Ressourcen so in einer logischen Sicht zusammenfasst, dass ihre Auslastung optimiert wird und sie Anforderungen automatisch zur Verfügung stehen. Der Lösungsansatz besteht in der Verknüpfung von Servern, Speichern und Netzen zu einem virtuellen Gesamtsystem, aus welchem die Anwendungen direkt und bedarfsgerecht ihre Ressourcen beziehen.
Man muss unterscheiden zwischen der Virtualisierung von Hardware, die sich mit der Verwaltung von Hardware-Ressourcen, beschäftigt, und dieVirtualisierung von Software, die sich mit der Verwaltung von Software-Ressourcen, wie z. B. Anwendungen und Betriebssystemen beschäftigt.},
